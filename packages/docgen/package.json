    "aiql",
    "documentation",
    "generator",
    "semantic"
  ],
  "author": "AIQL Contributors",
  "license": "MIT",
  "dependencies": {
    "@aiql-org/core": "^2.8.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
