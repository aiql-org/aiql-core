{
  "name": "aiql-sdk",
  "version": "2.8.0",
  "private": true,
  "description": "AIQL SDK - AI Query Language transpilers, tools, and runtime packages",
  "workspaces": [
    "packages/core",
    "packages/inference",
    "packages/security",
    "packages/semantics",
    "packages/soul",
    "packages/cli",
    "packages/api",
    "packages/utils",
    "packages/examples",
    "packages/playground"
  ],
  "scripts": {
    "build": "npm run lint && npm run build -w packages/core && npm run build -w packages/security && npm run build -w packages/inference && npm run build -w packages/soul && npm run build -w packages/semantics && npm run build -w packages/utils && npm run build -w packages/cli && npm run build -w packages/api && npm run build -w packages/examples && npm run build -w packages/playground",
    "test": "npm run test --workspaces --if-present",
    "test:examples": "node scripts/verify-examples.js",
    "clean": "npm run clean --workspaces --if-present",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "lint:workspaces": "npm run lint --workspaces --if-present",
    "sync-versions": "node scripts/sync-versions.js",
    "version-packages": "changeset version",
    "release": "npm run build && changeset publish",
    "docker:cli": "docker build -f docker/cli.Dockerfile -t aiql/cli:latest .",
    "docker:api": "docker build -f docker/api.Dockerfile -t aiql/api:latest .",
    "docker:playground": "docker build -f docker/playground.Dockerfile -t aiql/playground:latest .",
    "docker:all": "npm run docker:cli && npm run docker:api && npm run docker:playground"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aiql-org/aiql-core.git"
  },
  "keywords": [
    "aiql",
    "agi",
    "transpiler",
    "semantic-graph",
    "knowledge-representation",
    "logic",
    "inference",
    "quantum-computing",
    "sdk"
  ],
  "author": "AIQL Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aiql-org/aiql-core/issues"
  },
  "homepage": "https://aiql.org",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
